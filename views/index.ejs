<!DOCTYPE html>
<html lang="en" class="h-100">
<head>
  <%- include('./layout/head.ejs'); %>
  <style>
      .bd-placeholder-img {
          font-size: 1.125rem;
          text-anchor: middle;
          -webkit-user-select: none;
          -moz-user-select: none;
          user-select: none;
      }
      @media (min-width: 768px) {
          .bd-placeholder-img-lg {
          font-size: 3.5rem;
          }
      }
  </style>
  <title>Document</title>
</head>
<body>

    <!-- Header Section -->
    <% include('./layout/header.ejs') %> 

    <!-- Landing Section -->
    <section>
      <% include('./widget/landing') %> 
    </section>

    <main>

      <!-- Categories Section -->
      <% include('./widget/category.ejs') %> 

      <!-- Products Section -->
      <% include('./widget/product-home.ejs') %> 

        <section>
            <div class="container-lg py-3">
              <div class="row mb-2">
                <header class="d-flex justify-content-center mb-lg-4">
                  <h1 class="text-primary fw-bold d-inline-block"><span class="text-dark"> Products </span></h1>
                </header>
              </div>
              <div class="row mb-2">
                  <div class="col-md-6">
                    <div class="row g-0 border rounded overflow-hidden flex-md-row mb-4 shadow-sm h-md-250 position-relative">
                      <div class="col p-4 d-flex flex-column position-static">
                        <strong class="d-inline-block mb-2 text-primary">World</strong>
                        <h3 class="mb-0">Featured post</h3>
                        <div class="mb-1 text-muted">Nov 12</div>
                        <p class="card-text mb-auto">This is a wider card with supporting text below as a natural lead-in to additional content.</p>
                        <a href="#" class="stretched-link">Continue reading</a>
                      </div>
                      <div class="col-auto d-none d-lg-block">
                        <svg class="bd-placeholder-img" width="200" height="250" xmlns="http://www.w3.org/2000/svg" role="img" aria-label="Placeholder: Thumbnail" preserveAspectRatio="xMidYMid slice" focusable="false"><title>Placeholder</title><rect width="100%" height="100%" fill="#55595c"></rect><text x="50%" y="50%" fill="#eceeef" dy=".3em">Thumbnail</text></svg>
                      </div>
                    </div>
                  </div>
                  <div class="col-md-6">
                    <div class="row g-0 border rounded overflow-hidden flex-md-row mb-4 shadow-sm h-md-250 position-relative">
                      <div class="col p-4 d-flex flex-column position-static">
                        <strong class="d-inline-block mb-2 text-success">Design</strong>
                        <h3 class="mb-0">Post title</h3>
                        <div class="mb-1 text-muted">Nov 11</div>
                        <p class="mb-auto">This is a wider card with supporting text below as a natural lead-in to additional content.</p>
                        <a href="#" class="stretched-link">Continue reading</a>
                      </div>
                      <div class="col-auto d-none d-lg-block">
                        <svg class="bd-placeholder-img" width="200" height="250" xmlns="http://www.w3.org/2000/svg" role="img" aria-label="Placeholder: Thumbnail" preserveAspectRatio="xMidYMid slice" focusable="false"><title>Placeholder</title><rect width="100%" height="100%" fill="#55595c"></rect><text x="50%" y="50%" fill="#eceeef" dy=".3em">Thumbnail</text></svg>
              
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="d-flex justify-content-center my-4">
                        <button class="btn btn-outline-primary rounded-pill text-capitalize text"><a href="">More Products</a></button>
                    </div>
                  </div>
              </div>
            </div>
        </section>

    </main>
    
    <!-- Footer Section -->
    <%- include('./layout/footer.ejs'); %>
    
    <!-- Script File -->
    <%- include('./layout/script.ejs'); %>
</body>
</html>